<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.machine.service.data.mesage.dao.mapper.DataMqDeadMessageMapper">

    <insert id="insertByReliableMessageId">
        INSERT INTO t_data_mq_dead_message
        SELECT * FROM t_data_mq_reliable_message
        where id = #{reliableMessageId}
    </insert>

</mapper>
