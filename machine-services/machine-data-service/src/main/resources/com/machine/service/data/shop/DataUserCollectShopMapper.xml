<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.machine.service.data.shop.dao.mapper.DataUserCollectShopMapper">

    <select id="listCollectedIdByShopIdSet" resultType="java.lang.String">
        SELECT ucs.shop_id FROM t_data_user_collect_shop ucs
        WHERE ucs.user_id = #{userId}
        AND ucs.shop_id IN
        <foreach item="item" collection="shopIdSet" open="(" separator="," close=")">
            #{item}
        </foreach>
    </select>

</mapper>