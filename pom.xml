                <groupId>org.camunda.bpm</groupId>
                <artifactId>camunda-bom</artifactId>
                <version>7.24.0-alpha2</version>
                <type>pom</type>
                <scope>import</scope>
            </dependency>
            <!-- camunda 结束 -->

            <!-- swagger 开始 -->
