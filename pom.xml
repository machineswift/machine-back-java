            </dependency>
            <!-- swagger 结束 -->

            <!-- mybatis-plus 开始-->
            <dependency>
                <groupId>com.baomidou</groupId>
                <artifactId>mybatis-plus-spring-boot3-starter</artifactId>
                <version>3.5.14</version>
            </dependency>

            <dependency>
                <groupId>com.baomidou</groupId>
